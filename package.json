{
  "name": "agriresolve-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "nodemon --watch server --ext ts,json --exec \"tsx server/index.ts\"",
    "prebuild": "node scripts/generate-og-preview.mjs",
    "build": "vite build",
    "build:server": "tsc --project server/tsconfig.json",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --report-unused-disable-directives",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "start:server": "node dist/server/index.js",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:pbt": "jest --config jest.config.cjs --testNamePattern='Property:'",
    "coverage": "jest --config jest.config.cjs --coverage",
    "test:css": "stylelint \"src/**/*.css\""
  },
  "dependencies": {
    "@capacitor/android": "^8.0.2",
    "@capacitor/cli": "^8.0.2",
    "@capacitor/core": "^8.0.2",
    "@google/genai": "^1.34.0",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@socket.io/redis-adapter": "^8.2.1",
    "@types/express-session": "^1.18.2",
    "@types/three": "^0.182.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.19.0",
    "framer-motion": "^12.23.26",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^2.0.6",
    "i18next": "^25.7.3",
    "idb-keyval": "^6.2.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.562.0",
    "maath": "^0.10.8",
    "pg": "^8.11.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.13.0",
    "redis": "^4.6.10",
    "simple-peer": "^9.11.1",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "tailwind-merge": "^3.4.0",
    "three": "^0.182.0",
    "three-stdlib": "^2.36.1",
    "uuid": "^9.0.1",
    "ws": "^8.14.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^22.14.0",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "typescript": "~5.8.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.23",
    "concurrently": "^8.2.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
