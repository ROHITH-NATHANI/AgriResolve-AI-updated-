    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "sharp": "^0.33.5",
    "stylelint": "^16.14.1",
    "stylelint-config-standard": "^37.0.0",
    "supertest": "^7.2.2",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.21.0",
    "vite": "^6.2.0",
    "vite-plugin-pwa": "^1.2.0"
  }
}